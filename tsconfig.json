{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "jsx": "react",
    "resolveJsonModule": true,
    "baseUrl": ".",
    "paths": {
      "*": ["src/*"],
      "@components": ["src/components/*"],
      "@config": ["src/config/*"],
      "@assets": ["src/assets/*"],
      "@models": ["models/*"],
      "@theme": ["src/theme/*"],
      "@screens": ["src/screens/*"],
      "@i18n": ["src/i18n/*"],
      "@services": ["src/services/*"],
      "@types": ["src/types/*"],
      "@utils": ["src/utils/*"],
      "@navigators": ["src/navigators/*"],
    }
  }
}
